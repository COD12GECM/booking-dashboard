                  </button>
                  <button onclick="changeMonth(1)">
                    <svg style="width: 16px; height: 16px;" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"/></svg>
                  </button>
                </div>
              </div>
              <div class="mini-calendar-grid" id="calendarGrid">
                <!-- Calendar will be rendered by JS -->
              </div>
            </div>
            
            <!-- Recent Activity -->
            <div class="card">
              <div class="card-header">
                <h4 class="card-title">Recent Activity</h4>
                <a href="/dashboard?tab=bookings" class="btn btn-ghost btn-sm">View All</a>
